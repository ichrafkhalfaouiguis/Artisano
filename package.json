{
  "name": "romisartsshop",
  "version": "2.0.0",
  "description": "eCommerce application build  with mern stack",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d",
    "build": "npm install && npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Ichraf khalfaoui",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.4",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "colors": "^1.4.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.8",
    "react-google-login": "^5.2.2",
    "react-helmet-async": "^2.0.3",
    "react-paypal-js": "^0.0.1-security"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "nodemon": "^3.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ichrafkhalfaouiguis/RomisArtsShop.git"
  },
  "bugs": {
    "url": "https://github.com/ichrafkhalfaouiguis/RomisArtsShop/issues"
  },
  "homepage": "https://github.com/ichrafkhalfaouiguis/RomisArtsShop#readme"
}
